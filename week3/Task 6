#6.1
def gcd(a, b):
    while b:
        a, b = b, a % b
    return a
a = int(input())
b = int(input())
GCD = gcd(a, b)
LCM = (a * b) // GCD
print(GCD)
print(LCM)

#6.2
a = float(input())
b = float(input())
c = float(input())
d = float(input())
f = float(input())
p1 = (a + b + f) / 2
s1 = (p1 * (p1 - a) * (p1 - b) * (p1 - f)) ** 0.5
p2 = (c + d + f) / 2
s2 = (p2 * (p2 - c) * (p2 - d) * (p2 - f)) ** 0.5
print(s1 + s2)
